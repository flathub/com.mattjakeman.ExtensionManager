{
    "id" : "com.mattjakeman.ExtensionManager",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "49",
    "sdk" : "org.gnome.Sdk",
    "command" : "extension-manager",
    "finish-args" : [
        "--share=network",
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland",
        "--talk-name=org.gnome.Shell.Extensions",
        "--talk-name=org.gnome.SessionManager"
    ],
    "cleanup" : [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules" : [
        {
            "name" : "libbacktrace",
            "sources" : [
                {
                    "type" : "git",
                    "url" : "https://github.com/ianlancetaylor/libbacktrace.git",
                    "commit" : "793921876c981ce49759114d7bb89bb89b2d3a2d"
                }
            ]
        },
        {
            "name" : "extension-manager",
            "builddir" : true,
            "buildsystem" : "meson",
            "build-options" : {
                "no-debuginfo" : true
            },
            "config-opts" : [
                "-Ddevelopment=false",
                "-Dpackage=Flatpak",
                "-Ddistributor=mjakeman",
                "-Dofficial=true"
            ],
            "sources" : [
                {
                    "type" : "git",
                    "url" : "https://github.com/mjakeman/extension-manager",
                    "commit" : "7dfba11e4e92edbff24632e912cfb76f38ab776b",
                    "tag" : "v0.6.3"
                }
            ]
        }
    ]
}
